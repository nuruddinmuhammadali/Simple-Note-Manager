<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.notes.iit.simplenotesmanager">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/icon_note_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/icon_note_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ProfileUpdate"></activity>
        <activity
            android:name=".LoginActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".RegisterActivity"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".NotesListActivity" />
        <activity android:name=".ListEditActivity" />
    </application>

</manifest>